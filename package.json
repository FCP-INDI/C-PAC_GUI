{
  "name": "c-pac_gui",
  "description": "Configurable Pipeline for the Analysis of Connectomes",
  "homepage": "https://github.com/FCP-INDI/C-PAC_GUI",
  "productName": "C-PAC",
  "version": "0.0.2",
  "scripts": {
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --mode development --config config/webpack.config.renderer.dev.dll.js --colors",
    "build:electron": "cross-env node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --mode production --config config/webpack.config.main.prod.js --colors",
    "build:renderer": "cross-env node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --mode production --config config/webpack.config.renderer.prod.js --colors",
    "build:all": "yarn run build:electron && yarn run build:renderer",
    "electron-rebuild": "electron-rebuild --parallel --force --types prod,dev,optional --module-dir app",
    "postinstall": "yarn run build-dll && electron-builder install-app-deps && node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "prestart": "yarn run build",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "dev:browser": "TARGET=browser yarn run dev:renderer",
    "dev:electron": "npm-run-all --parallel dev:renderer dev:electron-main",
    "dev:electron-main": "cross-env NODE_ENV=development node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --mode development --config config/webpack.config.main.dev.js && cross-env HOT=1 NODE_ENV=development node_modules/electron/dist/electron -r @babel/register ./app/dist/main",
    "dev:renderer": "cross-env NODE_ENV=development node --trace-warnings -r @babel/register ./node_modules/webpack-dev-server/bin/webpack-dev-server --mode development --config config/webpack.config.renderer.dev.js",
    "lint": "cross-env NODE_ENV=development eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint:fix": "yarn run lint -- --fix",
    "lint-styles": "stylelint app/*.css app/components/*.css --syntax scss",
    "lint-styles:fix": "stylefmt -r app/*.css app/components/*.css",
    "package": "yarn run package:all",
    "publish": "publish linux-x64",
    "package:all": "yarn run build:all && build --publish never -ml",
    "package:linux": "TARGET=electron yarn run build:all && build --publish never --linux",
    "package:mac": "TARGET=electron yarn run build:all && build --publish never --mac",
    "clean:all": "yarn run clean:deps && yarn run clean:build",
    "clean:deps": "rm -Rf node_modules c-pac/node_modules",
    "clean:build": "rm -Rf app/dist dist"
  },
  "build": {
    "productName": "C-PAC",
    "appId": "org.fcp-indi.CPAC",
    "files": [
      "dist/main.js",
      "node_modules/",
      "c-pac/",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "deb",
        "snap",
        "tar.gz"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "dist"
    }
  },
  "license": "MIT",
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "redux",
    "sass",
    "webpack",
    "hot",
    "reload"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "concurrently": "^4.0.1",
    "cross-env": "^5.1.3",
    "cross-spawn": "^6.0.4",
    "css-loader": "^1.0.0",
    "detect-port": "^1.2.2",
    "electron": "^3.0.4",
    "electron-builder": "^20.28.4",
    "electron-devtools-installer": "^2.2.3",
    "electron-rebuild": "^1.7.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.6.1",
    "fbjs-scripts": "^1.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "mini-css-extract-plugin": "^0.4.4",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.7.2",
    "npm-logical-tree": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^1.0.0",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "sinon": "^6.3.5",
    "spectron": "^5.0.0",
    "speed-measure-webpack-plugin": "^1.2.3",
    "style-loader": "^0.23.1",
    "stylefmt": "^6.0.0",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.0.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "@bugsnag/js": "^5.1.0",
    "@bugsnag/plugin-react": "^5.1.0",
    "@internal/c-pac": "file:./c-pac",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "devtron": "^1.4.0",
    "electron-args": "^0.1.0",
    "electron-debug": "^2.0.0",
    "electron-simple-publisher": "^0.6.1",
    "electron-store": "^2.0.0",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.12",
    "ini": "^1.3.5",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.set": "^4.3.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-ga": "^2.5.7",
    "react-hot-loader": "^4.3.12",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-syntax-highlighter": "^10.1.2",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "source-map-support": "^0.5.3",
    "uuid": "^3.3.2"
  },
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  }
}
