{
  "name": "c-pac_gui",
  "description": "Configurable Pipeline for the Analysis of Connectomes",
  "homepage": "https://github.com/FCP-INDI/C-PAC_GUI",
  "productName": "C-PAC",
  "version": "0.0.12",
  "scripts": {
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings --trace-deprecation -r @babel/register ./node_modules/webpack/bin/webpack --mode development --config config/webpack.config.renderer.dev.dll.js --color",
    "build:electron": "cross-env node --trace-warnings --trace-deprecation -r @babel/register ./node_modules/webpack/bin/webpack --mode production --config config/webpack.config.main.prod.js --color",
    "build:renderer": "cross-env node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --mode production --config config/webpack.config.renderer.prod.js --color",
    "build:all": "yarn run build:electron && yarn run build:renderer",
    "electron-rebuild": "rebuild --parallel --force --types prod,dev,optional --module-dir app",
    "postinstall": "yarn run build-dll && electron-builder install-app-deps",
    "prepare": "relative-deps",
    "prestart": "yarn run build:all",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "dev:browser": "TARGET=browser yarn run dev:renderer",
    "dev:electron": "npm-run-all --parallel dev:renderer dev:electron-main",
    "dev:electron-main": "cross-env NODE_ENV=development node --trace-warnings --trace-deprecation -r @babel/register ./node_modules/webpack/bin/webpack --mode development --config config/webpack.config.main.dev.js && cross-env HOT=1 NODE_ENV=development node_modules/electron/dist/electron -r @babel/register ./app/dist/main",
    "dev:renderer": "cross-env NODE_ENV=development node --trace-warnings --trace-deprecation -r @babel/register ./node_modules/webpack/bin/webpack serve --mode development --config config/webpack.config.renderer.dev.js",
    "lint": "cross-env NODE_ENV=development eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint:fix": "yarn run lint -- --fix",
    "lint-styles": "stylelint app/*.css --syntax scss",
    "lint-styles:fix": "stylelint --fix -r app/*.css",
    "package": "yarn run package:all",
    "publish": "publish linux-x64",
    "package:all": "yarn run build:all && electron-builder --publish never -ml",
    "package:linux": "TARGET=electron yarn run build:all && electron-builder --publish never --linux",
    "package:mac": "TARGET=electron yarn run build:all && electron-builder --publish never --mac",
    "clean:all": "yarn run clean:deps && yarn run clean:build",
    "clean:deps": "rm -Rf node_modules c-pac/node_modules",
    "clean:build": "rm -Rf app/dist dist"
  },
  "build": {
    "productName": "C-PAC",
    "appId": "org.fcp-indi.CPAC",
    "files": [
      "dist/main.js",
      "node_modules/",
      "c-pac/",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "deb",
        "snap",
        "tar.gz"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "dist"
    }
  },
  "license": "MIT",
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "redux",
    "sass",
    "webpack",
    "hot",
    "reload"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/eslint-plugin": "^7.13.10",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-proposal-do-expressions": "^7.25.9",
    "@babel/plugin-proposal-export-default-from": "^7.25.9",
    "@babel/plugin-proposal-function-bind": "^7.25.9",
    "@babel/plugin-proposal-function-sent": "^7.25.9",
    "@babel/plugin-proposal-pipeline-operator": "^7.25.9",
    "@babel/plugin-proposal-throw-expressions": "^7.25.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-export-namespace-from": "^7.25.9",
    "@babel/plugin-transform-flow-strip-types": "^7.26.5",
    "@babel/plugin-transform-json-strings": "^7.25.9",
    "@babel/plugin-transform-logical-assignment-operators": "7.25.9",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.26.6",
    "@babel/plugin-transform-numeric-separator": "^7.25.9",
    "@babel/plugin-transform-optional-chaining": "^7.25.9",
    "@babel/plugin-transform-react-jsx-source": "^7.25.9",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@electron/rebuild": "^3.7.2",
    "@eslint/compat": "^1.2.8",
    "@types/react": "^19.1.2",
    "babel-loader": "^10.0.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-inline-import": "^3.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "^5.2.0",
    "electron": "^35.1.5",
    "electron-builder": "^26.0.12",
    "electron-devtools-installer": "^4.0.0",
    "electron-simple-publisher": "^2.0.1",
    "eslint": "^9.24.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-webpack-plugin": "^5.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": ">=2.9.2",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "redux-logger": "^3.0.6",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^0.23.1",
    "stylelint": "^16.18.0",
    "stylelint-config-standard": "^38.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.30.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@bugsnag/browser": "*",
    "@bugsnag/js": "^8.2.0",
    "@bugsnag/plugin-react": "^8.2.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@internal/c-pac": "workspace:./c-pac",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@mui/styled-engine-sc": "^7.0.2",
    "@mui/styles": "6.4.8",
    "clsx": "^1.0.4",
    "electron-args": "^0.1.0",
    "electron-debug": "^2.0.0",
    "electron-store": "^6.0.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "font-awesome": "^4.7.0",
    "global": ">=4.4.0",
    "globals": ">=13.12.1",
    "history": "^5.3.0",
    "immutable": "^4.0.0-rc.12",
    "ini": "^1.3.6",
    "memoize-one": "^5.2.1",
    "prismjs": "^1.3.0",
    "prop-types": "^15.7.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-ga": "^3.3.1",
    "react-mathjax": "^1.0.1",
    "react-redux": "^7.1.0",
    "react-refresh": "^0.10.0",
    "react-router": "^7.5.2",
    "react-router-dom": "^7.5.2",
    "react-syntax-highlighter": "^15.4.3",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.5",
    "source-map-support": "^0.5.3",
    "styled-components": "^6.1.17",
    "uuid": ">=11.1.0",
    "whatwg-fetch": "^3.0.0",
    "yarn": "^1.22.0"
  },
  "resolutions": {
    "glob": "11.0.1",
    "make-fetch-happen": "^14.0.3",
    "node-ssh": "^13.2.1",
    "prismjs": "^1.30.0",
    "url": "^0.11.0"
  },
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  },
  "packageManager": "yarn@4.9.1",
  "workspaces": [
    ".",
    "c-pac"
  ]
}
